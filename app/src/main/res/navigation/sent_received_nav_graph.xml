        <action
            android:id="@+id/action_sentEnviteFragment2_to_receivedEnviteFragment2"
            app:destination="@id/receivedEnviteFragment2"
            app:popUpTo="@id/receivedEnviteFragment2" />
    </fragment>
</navigation>