        <action
            android:id="@+id/action_receivedEnviteFragment2_to_sentEnviteFragment2"
            app:destination="@id/sentEnviteFragment2"
            app:popUpTo="@id/sentEnviteFragment2" />
    </fragment>
    <fragment
        android:id="@+id/sentEnviteFragment2"
        android:name="fragments.SentEnviteFragment"
        android:label="fragment_sent_envite"
